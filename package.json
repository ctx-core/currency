{
	"name": "@ctx-core/currency",
	"version": "10.0.36",
	"description": "ctx-core currency",
	"keywords": [
		"ctx-core",
		"currency"
	],
	"homepage": "https://github.com/ctx-core/currency#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/currency/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/currency.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
