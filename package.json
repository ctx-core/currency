	},
	"dependencies": {
		"@ctx-core/function": "workspace:^20.2.14",
		"@ctx-core/number": "workspace:^8.0.28",
		"@ctx-core/object": "workspace:^22.0.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./dist/index.js"
}